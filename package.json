{
  "name": "font-size-changer",
  "main": "./lib/font-size-changer",
  "version": "1.0.0",
  "description": "A statusBar tool to easily and quickly change font size",
  "keywords": [
    "font",
    "size",
    "fontsize",
    "statusBar",
    "font-size"
  ],
  "activationCommands": {
    "atom-workspace": "font-size-changer:toggle"
  },
  "dependencies": {},
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "repository": "https://github.com/Folded-Code/font-size-changer",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  }
}
